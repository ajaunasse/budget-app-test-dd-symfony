<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Core\Transaction\Domain\Model\Category" table="category">
        <id name="id" type="category_id" column="id">
            <generator strategy="AUTO"/>
            <sequence-generator sequence-name="category_seq" allocation-size="1" initial-value="1" />
        </id>
        <field name="name" column="name" type="string" length="50" />
    </entity>

</doctrine-mapping>